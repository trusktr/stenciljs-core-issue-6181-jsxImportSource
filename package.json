{
  "name": "kitchen-sink-stencil",
  "private": true,
  "version": "0.0.1",
  "description": "Minimal example showing how to write and use a custom element made with Lume in a Stencil app.",
  "scripts": {
    "// Note, `rm-rf`ing to clear cache each time to avoid stale node_modules build": "",
    "build": "rm -rf .stencil www/build && stencil build",
    "start": "rm -rf .stencil www/build && stencil build --dev --watch --serve",
    "generate": "rm -rf .stencil www/build && stencil generate"
  },
  "devDependencies": {
    "@lume/element": "../..",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@stencil/core": "4.22.2",
    "@types/jest": "^29.5.6",
    "classy-solid": "^0.4.3",
    "jest": "^29.7.0",
    "patch-package": "^8.0.0",
    "solid-js": "^1.9.5",
    "typescript": "^5.7.3"
  },
  "license": "MIT"
}
